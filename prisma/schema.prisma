generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model news {
  id         Int      @id @default(autoincrement())
  date       DateTime @db.Timestamp(6)
  name_cz    String   @db.VarChar(255)
  name_en    String   @db.VarChar(255)
  content_cz String
  content_en String
  uuid       String   @unique @db.VarChar(255)
}

model user {
  id       Int    @id @default(autoincrement())
  login    String @db.VarChar(255)
  password String @db.VarChar(255)
}

model employee {
  id                Int     @id @default(autoincrement())
  name              String  @db.VarChar(255)
  status            String? @db.VarChar(10)
  degree            String? @db.VarChar(10)
  position_cz       String? @db.VarChar(255)
  email             String? @db.VarChar(255)
  phone             String? @db.VarChar(255)
  place_cz          String? @db.VarChar(255)
  consultation_cz   String? @db.VarChar(255)
  specialization_cz String? @db.VarChar(255)
  type              String? @db.VarChar(50)
  studyType         String? @db.VarChar(255)
  position_en       String? @db.VarChar(255)
  place_en          String? @db.VarChar(255)
  consultation_en   String? @db.VarChar(255)
  specialization_en String? @db.VarChar(255)
  uuid              String  @unique @db.VarChar(255)
  order             Int     @unique
}
